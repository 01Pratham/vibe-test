{
    "name": "restiqo",
    "version": "1.0.3",
    "description": "Auto-generating Postman-like UI for Express applications",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "ui",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build:lib": "tsc",
        "build:ui": "cd ui-src && npm run build",
        "build": "npm run build:lib && npm run build:ui",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "lint:ui": "cd ui-src && npm run lint",
        "lint:all": "npm run lint && npm run lint:ui",
        "version": "npm version $npm_package_version --prefix ui-src --no-git-tag-version && git add ui-src/package.json",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "express",
        "api",
        "testing",
        "postman",
        "swagger",
        "rest",
        "api-testing",
        "api-documentation",
        "express-middleware",
        "api-tester",
        "http-client",
        "request-testing",
        "api-explorer",
        "developer-tools"
    ],
    "author": {
        "name": "Prathamesh Chavan",
        "email": "cprathmesh01@gmail.com",
        "url": "https://github.com/01Pratham"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/01Pratham/restiqo.git"
    },
    "bugs": {
        "url": "https://github.com/01Pratham/restiqo/issues"
    },
    "homepage": "https://github.com/01Pratham/restiqo#readme",
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/01Pratham"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {},
    "peerDependencies": {
        "express": "^4.18.2",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.16"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.32.0",
        "express": "^4.21.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.55.0"
    }
}
